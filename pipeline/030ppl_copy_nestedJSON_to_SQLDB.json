{
	"name": "030ppl_copy_nestedJSON_to_SQLDB",
	"properties": {
		"activities": [
			{
				"name": "Copy JSON to SQLDB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['EmployeeKey']"
								},
								"sink": {
									"name": "EmployeeID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['EmpName']"
								},
								"sink": {
									"name": "EmpName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Contact']['Email']"
								},
								"sink": {
									"name": "Email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Contact']['Phone']"
								},
								"sink": {
									"name": "Contact",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_NJSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_JSONempnew",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-12-21T00:18:30Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}